{
  "name": "osnap",
  "version": "0.0.1",
  "description": "A Snapshot tool",
  "license": "MIT",
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildDev": "dune build --promote-install-files --root . --only-package #{self.name}",
    "release": {
      "bin": {
        "osnap": "osnap",
        "downloadChromium": "downloadChromium"
      },
      "includePackages": [
        "osnap",
        "esy-libpng",
        "esy-zlib",
        "esy-gmp"
      ]
    }
  },
  "scripts": {
    "run": "esy x osnap"
  },
  "dependencies": {
    "@opam/base64": "3.5.0",
    "@opam/camlimages": "*",
    "@opam/camlzip": "*",
    "@opam/cmdliner": "1.0.4",
    "@opam/cohttp": "4.0.0",
    "@opam/cohttp-lwt-unix": "4.0.0",
    "@opam/conf-gmp-powm-sec": "2",
    "@opam/fileutils": "0.6.3",
    "@opam/lwt": "5.4.0",
    "@opam/lwt_ppx": "2.0.1",
    "@opam/ocamlfind": "1.8.1",
    "@opam/ppx_yojson_conv": "v0.14.0",
    "@opam/re": "*",
    "@opam/tls": "0.12.6",
    "@opam/uri": "4.1.0",
    "@opam/websocket": "2.14",
    "@opam/websocket-lwt-unix": "2.14",
    "@opam/yojson": "1.7.0",
    "@opam/zarith": "1.11",
    "@reason-native/console": "*",
    "@reason-native/pastel": "*",
    "esy-libpng": "*",
    "esy-zlib": "*",
    "esy-gmp": "*",
    "odiff": "*"
  },
  "devDependencies": {
    "@opam/reason": ">= 3.6.0 < 4.0.0",
    "@opam/dune": "< 3.0.0",
    "@opam/ocaml-lsp-server": "*",
    "ocaml": ">= 4.2.0 < 4.11.0"
  },
  "resolutions": {
    "@opam/re": "ocaml/ocaml-re:re.opam#09c2745a2d8d1517b3d597396e82e122903b0017",
    "@opam/camlzip": {
      "source": "rvantonder/camlzip:camlzip.opam#6b7ed9c8537d23b9f1781257f4f978ea0f493195",
      "override": {
        "dependencies": {
          "@opam/dune": "< 3.0.0",
          "@opam/dune-configurator": "< 3.0.0",
          "@opam/conf-zlib": "*"
        }
      }
    },
    "@opam/camlimages": {
      "source": "git+https://gitlab.com/camlspotter/camlimages.git:camlimages.opam#9d8dce704a335b7c568e8d3692fe086b1d084870",
      "override": {
        "dependencies": {
          "esy-libpng": "*"
        }
      }
    },
    "esy-zlib": "esy-packages/esy-zlib#404929fd8b7ed83ed6a528d751840faff957b4b3",
    "esy-gmp": "esy-packages/esy-gmp#e27cb300adfb0c0b320c273082c5affafcd225fa",
    "esy-libpng": "dmtrKovalenko/esy-libpng#a8f4da71d87e5bfbf567f472c0e52c95db90e04d",
    "odiff": "ewert-online/odiff#0bb2f2cd16fe6d64b16414d106d4c46c92b6ecfd"
  }
}
