{
  "name": "@space-labs/osnap",
  "version": "1.0.0-alpha+1",
  "description": "A Snapshot testing tool focusing on speed and ease of use.",
  "license": "MIT",
  "esy": {
    "build": "dune build -p osnap",
    "buildDev": "dune build --promote-install-files --root . --only-package osnap",
    "release": {
      "bin": {
        "osnap": "osnap",
        "downloadChromium": "downloadChromium"
      },
      "includePackages": [
        "root",
        "esy-libpng"
      ]
    }
  },
  "scripts": {
    "run": "esy x osnap",
    "test": "esy x osnap --config ./test/osnap.config.json"
  },
  "dependencies": {
    "@opam/base64": "3.5.0",
    "@opam/camlimages": "*",
    "@opam/camlzip": "*",
    "@opam/cmdliner": "1.0.4",
    "@opam/conf-gmp-powm-sec": "2",
    "@opam/fileutils": "0.6.3",
    "@opam/httpaf": "0.7.1",
    "@opam/httpaf-lwt-unix": "0.7.1",
    "@opam/lwt": "5.4.0",
    "@opam/lwt_ppx": "2.0.1",
    "@opam/ppx_yojson_conv": "v0.14.0",
    "@opam/re": "*",
    "@opam/uri": "4.1.0",
    "@opam/websocket": "2.14",
    "@opam/websocket-lwt-unix": "2.14",
    "@opam/yojson": "1.7.0",
    "@opam/zarith": "1.11",
    "@reason-native/console": "jfrolich/reason-native:console.opam#3e338dcfdb91416a22758a819b2d64f472a34e1c",
    "@reason-native/pastel": "jfrolich/reason-native:pastel.opam#3e338dcfdb91416a22758a819b2d64f472a34e1c",
    "esy-libpng": "*",
    "odiff-core": "ewert-online/odiff:odiff-core.opam#43859385306fed40146264aebb8b8faf941e6a87"
  },
  "devDependencies": {
    "@opam/reason": ">= 3.6.0 < 4.0.0",
    "@opam/dune": "< 3.0.0",
    "@opam/ocaml-lsp-server": "*",
    "ocaml": ">= 4.2.0 < 5.0.0"
  },
  "resolutions": {
    "@opam/camlimages": {
      "source": "git+https://gitlab.com/camlspotter/camlimages.git:camlimages.opam#9d8dce704a335b7c568e8d3692fe086b1d084870",
      "override": {
        "dependencies": {
          "esy-libpng": "*"
        }
      }
    },
    "@opam/camlzip": {
      "source": "rvantonder/camlzip:camlzip.opam#6b7ed9c8537d23b9f1781257f4f978ea0f493195",
      "override": {
        "dependencies": {
          "@opam/dune": "< 3.0.0",
          "@opam/dune-configurator": "< 3.0.0",
          "@opam/conf-zlib": "*"
        }
      }
    },
    "@opam/re": "ocaml/ocaml-re:re.opam#09c2745a2d8d1517b3d597396e82e122903b0017",
    "esy-libpng": "esy-packages/esy-libpng#d846df4a8547b3299c7627773008bd97543d7048"
  }
}
